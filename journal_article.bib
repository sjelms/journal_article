  month     =  jan,
  year      =  2014,
  url       = "http://dx.doi.org/10.1016/j.compedu.2013.07.033",
  keywords  = "\_XR/MR;Education;§New Literature;Journal Articles",
  language  = "en",
  issn      = "0360-1315, 1873-782X",
  doi       = "10.1016/j.compedu.2013.07.033"
}

